export LANG=ja_JP.UTF-8
export EDITOR=vim

# rbenv
export RBENV_ROOT="~/.rbenv"
export PATH="$RBENV_ROOT/bin:$PATH"
eval "$(rbenv init - zsh)"

# nodebrew
export PATH=$HOME/.nodebrew/current/bin:$PATH
unalias bower

# tmux. must be last.
if [ $SHLVL = 1 ]; then
  tmux a || tmux -f $HOME/.tmux.conf
fi

